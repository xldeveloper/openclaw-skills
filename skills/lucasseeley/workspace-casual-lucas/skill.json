{
  "name": "workspace-casual-lucas",
  "version": "1.0.1",
  "description": "Casual interface for OpenClaw workspace commands",
  "main": "index.js",
  "author": "LucasSeeley",
  "commands": [
    {
      "name": "list_workspace",
      "description": "List files in the OpenClaw workspace",
      "arguments": [
        {
          "name": "path",
          "type": "string",
          "default": "C:\\Users\\lucas\\OpenClawWorkspace"
        }
      ]
    },
    {
      "name": "run_command",
      "description": "Run a shell command",
      "arguments": [
        {
          "name": "command",
          "type": "string"
        }
      ]
    },
    {
      "name": "open_file",
      "description": "Open a specific file",
      "arguments": [
        {
          "name": "file",
          "type": "string"
        }
      ]
    }
  ]
}
